{
  "name": "deverobot",
  "version": "1.1.0",
  "description": "Deverobot for Discord",
  "main": "./lib/src/index.js",
  "license": "MIT",
  "repository": "https://github.com/kimmymonassar/DeveroBot.git",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node lib/src/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon lib/src/index.js\"",
    "test": "jest",
    "release": "standard-version",
    "release-patch": "standard-version -- --release-as patch",
    "release-as-minor": "standard-version -- --release-as minor",
    "release-as-pre": "standard-version -- --release-as prerelease",
    "release-as-major": "standard-version -- --release-as major"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^14.14.7",
    "@types/ws": "^7.4.0",
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "discord.js": "^12.4.1",
    "discord.js-commando": "git+https://github.com/discordjs/Commando.git",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.14",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "ytdl-core": "^4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
